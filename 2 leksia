import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
      Scanner num = new Scanner(System.in);
      int first, second;
        System.out.print("Enter first num: ");
        first = num.nextInt();
        System.out.print("Enter second num: ");
        second = num.nextInt();

        if (first != second)
            System.out.print(first + second);
        else
            System.out.print(first * second);

    }
}
